{
  "type": "elementList",
    "identifier": "sample",
  "elements": [
    {
      "identifier": "introduction",
      "type": "instruction",
      "title": "Baseline Survey",
      "text": "In order to help us gain a better understanding of your thoughts, feelings and experiences, please complete the following questions. It will take approximately 12 minutes to complete todayâ€™s portion and about 4-6 minutes per day (2-3 minutes in the morning and 2-3 minutes in the evening) for the next 21 days."
    },
    {
      "identifier": "gender",
      "type": "singleChoiceText",
      "text": "What is your gender?",
      "items": [
        {
          "prompt": "Female",
          "value": "female"
        },
        {
          "prompt": "Male",
          "value": "male"
        },
        {
          "prompt": "Transgender",
          "value": "transgender"
        },
        {
          "prompt": "Other",
          "value": "other"
        }
      ]
    },
    {
      "identifier": "age",
      "type": "numericInteger",
